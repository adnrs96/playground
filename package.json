{
  "name": "playground",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --coverage --runInBand",
    "test:integration-headless": "TEST_MODE=integration vue-cli-service test:unit --runInBand",
    "test:integration": "TEST_MODE=integration-local vue-cli-service test:unit --runInBand",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "release": "standard-version",
    "pwa-audit": "lighthouse-badges -s --urls https://dev.playground.storyscript.io/ -o .lighthouse-badges && lighthouse-badges --urls https://dev.playground.storyscript.io/ -o .lighthouse-badges",
    "percy": "percy exec -- yarn test:integration-headless",
    "stack": "docker-compose up --build -d",
    "stack:playground": "docker-compose up --build -d playground",
    "stack:coverage": "docker-compose up --build -d coverage"
  },
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "@sentry/integrations": "^5.6.1",
    "core-js": "^2.6.9",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@percy/puppeteer": "^1.0.8",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.11.2",
    "@types/puppeteer": "^1.20.1",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.8",
    "jest-puppeteer": "^4.3.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^9.4.1",
    "lodash": "^4.17.15",
    "perfect-scrollbar": "^1.4.0",
    "puppeteer": "^1.20.0",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "standard-version": "^7.0.0",
    "tailwindcss": "^1.1.3",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "vue-moment": "^4.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
